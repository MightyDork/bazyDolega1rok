use dolegae;
desc zasob;
select * from zasob where month(dataPozyskania) IN (7,8);
select * from zasob where rodzaj is not null order by waga asc;
desc kreatura;
select * from kreatura order by dataUr desc limit 5;
select distinct rodzaj from zasob;
SELECT CONCAT(nazwa, " ", rodzaj) AS polecenie
FROM kreatura where rodzaj regexp '^wi';
select nazwa, waga * ilosc as waga_totalna from zasob where year(dataPozyskania) between 2000 and 2007; 
select nazwa, waga * ilosc * 0.7 as waga_jadalna, waga * ilosc * 0.3 as waste from zasob where rodzaj='jedzenie';
select * from zasob where rodzaj is null;
select distinct nazwa, rodzaj from zasob where nazwa regexp'^Ba' or nazwa regexp'.*os$' order by nazwa asc;
select avg(waga) from kreatura where rodzaj='wiking';
select rodzaj, avg(waga), count(nazwa) from kreatura group by rodzaj;
select rodzaj, avg(2022-year(dataUr)) as sredni_wiek from kreatura group by rodzaj;
select * from kreatura;
select nazwa, avg(waga) from zasob group by nazwa having avg(waga) > 10 and sum(ilosc) >= 4;
desc zasob;
select nazwa, rodzaj, count(nazwa) from zasob group by nazwa, rodzaj having sum(ilosc) > 1;
select * from zasob order by rodzaj;
